function rollDice(numSides) {
  return Math.floor(Math.random() * numSides) + 1;
}

const maxNum = Number(prompt("Enter the maximum number on the dice:"));
document.write("<ul>");
let roll = rollDice(maxNum);
while (roll !== maxNum) {
  document.write(`<li>${roll}</li>`);
  roll = rollDice(maxNum);
}
document.write(`<li>${roll}</li>`);
document.write("</ul>");
